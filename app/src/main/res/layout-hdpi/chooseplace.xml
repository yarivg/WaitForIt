<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:background="@drawable/bigsizeclassicmenu">


    <ImageButton
        android:scaleType="fitXY"
        android:background="@android:color/transparent"
        android:layout_marginBottom="10dp"
        android:src="@drawable/nextbutton"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/nextbutton"
        android:layout_gravity="right|bottom"
        android:layout_marginRight="15dp"
        android:onClick="clickNext" />

    <ImageButton
        android:background="@android:color/transparent"
        android:layout_marginBottom="100dp"
        android:src="@drawable/govermentoffice"
        android:layout_width="@dimen/chooseplace_option_width"
        android:layout_height="@dimen/chooseplace_option_height"
        android:id="@+id/goverment"
        android:layout_gravity="center"
        android:scaleType="fitXY"
        android:onClick="clickCategory" />

    <ImageButton
        android:background="@android:color/transparent"
        android:layout_marginBottom="100dp"
        android:layout_marginLeft="100dp"
        android:src="@drawable/airport"
        android:layout_width="@dimen/chooseplace_option_width"
        android:layout_height="@dimen/chooseplace_option_height"
        android:id="@+id/airport"
        android:layout_gravity="center"
        android:scaleType="fitXY"
        android:onClick="clickCategory" />

    <ImageButton
        android:background="@android:color/transparent"
        android:layout_marginBottom="100dp"
        android:layout_marginRight="100dp"
        android:src="@drawable/busstation"
        android:layout_width="@dimen/chooseplace_option_width"
        android:layout_height="@dimen/chooseplace_option_height"
        android:id="@+id/bus"
        android:layout_gravity="center"
        android:scaleType="fitXY"
        android:onClick="clickCategory" />
    <ImageButton
        android:background="@android:color/transparent"
        android:layout_marginLeft="100dp"
        android:src="@drawable/clinic"
        android:layout_width="@dimen/chooseplace_option_width"
        android:layout_height="@dimen/chooseplace_option_height"
        android:id="@+id/clinic"
        android:layout_gravity="center"
        android:scaleType="fitXY"
        android:onClick="clickCategory"
        android:layout_marginBottom="5dp" />

    <ImageButton
        android:background="@android:color/transparent"
        android:layout_marginRight="100dp"
        android:src="@drawable/amusementpark"
        android:layout_width="@dimen/chooseplace_option_width"
        android:layout_height="@dimen/chooseplace_option_height"
        android:id="@+id/park"
        android:layout_gravity="center"
        android:scaleType="fitXY"
        android:layout_marginBottom="5dp" />
    <ImageButton
        android:background="@android:color/transparent"
        android:src="@drawable/trainstation"
        android:layout_width="@dimen/chooseplace_option_width"
        android:layout_height="@dimen/chooseplace_option_height"
        android:id="@+id/train"
        android:layout_gravity="center"
        android:scaleType="fitXY"
        android:layout_marginBottom="5dp" />

    <ImageButton
        android:background="@android:color/transparent"
        android:layout_marginTop="90dp"
        android:layout_marginRight="100dp"
        android:src="@drawable/restaurent"
        android:layout_width="@dimen/chooseplace_option_width"
        android:layout_height="@dimen/chooseplace_option_height"
        android:id="@+id/restaurent"
        android:layout_gravity="center"
        android:scaleType="fitXY"
        />
    <ImageButton
        android:background="@android:color/transparent"
        android:layout_marginTop="90dp"
        android:layout_marginLeft="100dp"
        android:src="@drawable/vehicle"
        android:layout_width="@dimen/chooseplace_option_width"
        android:layout_height="@dimen/chooseplace_option_height"
        android:id="@+id/vehicle"
        android:layout_gravity="center"
        android:scaleType="fitXY"
        />

    <ImageView
        android:layout_marginTop="65dp"
        android:layout_marginLeft="20dp"
        android:src="@drawable/whereareyouwaitingtitle"
        android:layout_width="180dp"
        android:layout_height="40dp"
        android:id="@+id/categorytitle"
        android:scaleType="fitXY"
        android:layout_gravity="left|top" />

    <ImageButton
        android:background="@android:color/transparent"
        android:src="@drawable/settings"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:id="@+id/settings1"
        android:scaleType="fitXY"
        android:layout_gravity="left|top"
        android:onClick="clicksetting" />

    <ImageButton
        android:background="@android:color/transparent"
        android:src="@drawable/postoffice"
        android:layout_width="@dimen/chooseplace_option_width"
        android:layout_height="@dimen/chooseplace_option_height"
        android:id="@+id/postoffice"
        android:layout_gravity="center"
        android:scaleType="fitXY"
        android:onClick="clickCategory"
        android:layout_marginTop="90dp" />

    <ImageButton
        android:scaleType="fitXY"
        android:background="@android:color/transparent"
        android:layout_marginBottom="10dp"
        android:src="@drawable/previousbutton"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/previousbutton"
        android:layout_gravity="left|bottom"
        android:onClick="clickNext"
        android:layout_marginLeft="15dp" />

    <ImageButton
        android:background="@android:color/transparent"
        android:src="@drawable/others"
        android:layout_width="@dimen/chooseplace_option_width"
        android:layout_height="@dimen/chooseplace_option_height"
        android:id="@+id/others"
        android:layout_gravity="center"
        android:scaleType="fitXY"
        android:onClick="clickCategory"
        android:layout_marginTop="185dp" />

</FrameLayout>
